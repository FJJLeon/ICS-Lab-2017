	.pos 0
	irmovq stack, %rsp
	call main
	halt

	.align 8
ele1:
	.quad 0x00a
	.quad ele2
ele2:
	.quad 0x0b0
	.quad ele3
ele3:
	.quad 0xc00
	.quad 0
main:
	irmovq ele1, %rdi
	call sum_list
	ret

#long sum_list(list_ptr ls)
#ls in %rdi
sum_list:    
	pushq %rbp
	rrmovq %rsp, %rbp
	xorq %rax, %rax
	irmovq $0, %r8
	subq %r8, %rdi
	je test
loop:
	mrmovq (%rdi), %r10
	addq %r10, %rax
	mrmovq 8(%rdi), %rdi
test:	
	subq %r8, %rdi
	jne loop
	popq %rbp
	ret
	
	.pos 0x200
stack:
